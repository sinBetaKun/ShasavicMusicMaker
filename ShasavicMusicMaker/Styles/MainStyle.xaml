<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="Border1" TargetType="Border">
        <Setter Property="BorderBrush" Value="#B5AFA5"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="4"/>
        <Setter Property="Background" Value="#F5EFEC"/>
    </Style>
    <Style x:Key="VerticalSeparator" TargetType="Border">
        <Setter Property="Background" Value="#B5AFA5"/>
        <Setter Property="Margin" Value="8,0,8,0"/>
        <Setter Property="Width" Value="1"/>
    </Style>
    <Style x:Key="Menu1" TargetType="Menu">
        <Setter Property="Background" Value="#FBFAF8"/>
        <Setter Property="BorderBrush" Value="#B5AFA5"/>
        <Setter Property="BorderThickness" Value="1"/>
    </Style>
</ResourceDictionary>
